#include<iostream>
using namespace std;

int main()
{
    int a[100],n,i;
    cout<<"Enter number you want to keep in array "<<"\n"
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"you have successfuly entered "<<n<<" number of element ";
    
    for(i=0;i<n;i++)
    {
        cout<<a[i]<<" "<<"\n";
    }
    
    return 0;
}
